    <sentence id="DDI-MedLine.d17.s1" text="The interaction of prostaglandin F2alpha and synthetic oxytocin on placental vessels was studied in vitro. ">
        <entity charOffset="19-39" id="DDI-MedLine.d17.s1.e0" text="prostaglandin F2alpha" type="drug" />
        <entity charOffset="55-62" id="DDI-MedLine.d17.s1.e1" text="oxytocin" type="drug" />
