    <sentence id="DDI-MedLine.d7.s1" text="This report describes two cases in which theophylline clearance accelerated markedly with concomitant phenytoin administration. ">
        <entity charOffset="41-52" id="DDI-MedLine.d7.s1.e0" text="theophylline" type="drug" />
        <entity charOffset="102-110" id="DDI-MedLine.d7.s1.e1" text="phenytoin" type="drug" />
