<document id="DDI-MedLine.d122">
    <sentence id="DDI-MedLine.d122.s1" text="Many people use both alcohol and nicotine (i.e., cigarettes and other tobacco products). ">
        <entity charOffset="21-27" id="DDI-MedLine.d122.s1.e0" text="alcohol" type="drug" />
        <entity charOffset="33-40" id="DDI-MedLine.d122.s1.e1" text="nicotine" type="drug" />
