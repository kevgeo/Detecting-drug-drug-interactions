<document id="DDI-MedLine.d3">
    <sentence id="DDI-MedLine.d3.s6" text="Rifabutin did not significantly affect amprenavir's pharmacokinetics. ">
        <entity charOffset="0-8" id="DDI-MedLine.d3.s6.e0" text="Rifabutin" type="drug" />
        <entity charOffset="39-48" id="DDI-MedLine.d3.s6.e1" text="amprenavir" type="drug" />
        <pair ddi="false" e1="DDI-MedLine.d3.s6.e0" e2="DDI-MedLine.d3.s6.e1" id="DDI-MedLine.d3.s6.p0" />
    </sentence>
    <sentence id="DDI-MedLine.d3.s9" text="Amprenavir decreased the results of the ERMBT by 83%. ">
        <entity charOffset="0-9" id="DDI-MedLine.d3.s9.e0" text="Amprenavir" type="drug" />
    </sentence>
    <sentence id="DDI-MedLine.d3.s10" text="The results of the ERMBT after 2 weeks of rifabutin and rifampin therapy were increased 187 and 156%, respectively. ">
        <entity charOffset="42-50" id="DDI-MedLine.d3.s10.e0" text="rifabutin" type="drug" />
        <entity charOffset="56-63" id="DDI-MedLine.d3.s10.e1" text="rifampin" type="drug" />
        <pair ddi="false" e1="DDI-MedLine.d3.s10.e0" e2="DDI-MedLine.d3.s10.e1" id="DDI-MedLine.d3.s10.p0" />
    </sentence>
    <sentence id="DDI-MedLine.d3.s11" text="Amprenavir plus rifampin was well tolerated. ">
        <entity charOffset="0-9" id="DDI-MedLine.d3.s11.e0" text="Amprenavir" type="drug" />
        <entity charOffset="16-23" id="DDI-MedLine.d3.s11.e1" text="rifampin" type="drug" />
        <pair ddi="false" e1="DDI-MedLine.d3.s11.e0" e2="DDI-MedLine.d3.s11.e1" id="DDI-MedLine.d3.s11.p0" />
    </sentence>
    <sentence id="DDI-MedLine.d3.s12" text="Amprenavir plus rifabutin was poorly tolerated, and 5 of 11 subjects discontinued therapy. ">
        <entity charOffset="0-9" id="DDI-MedLine.d3.s12.e0" text="Amprenavir" type="drug" />
        <entity charOffset="16-24" id="DDI-MedLine.d3.s12.e1" text="rifabutin" type="drug" />
        <pair ddi="true" e1="DDI-MedLine.d3.s12.e0" e2="DDI-MedLine.d3.s12.e1" id="DDI-MedLine.d3.s12.p0" type="effect" />
    </sentence>
    <sentence id="DDI-MedLine.d3.s13" text="Rifampin markedly increases the metabolic clearance of amprenavir, and coadministration is contraindicated. ">
        <entity charOffset="0-7" id="DDI-MedLine.d3.s13.e0" text="Rifampin" type="drug" />
        <entity charOffset="55-64" id="DDI-MedLine.d3.s13.e1" text="amprenavir" type="drug" />
        <pair ddi="true" e1="DDI-MedLine.d3.s13.e0" e2="DDI-MedLine.d3.s13.e1" id="DDI-MedLine.d3.s13.p0" type="mechanism" />
    </sentence>
