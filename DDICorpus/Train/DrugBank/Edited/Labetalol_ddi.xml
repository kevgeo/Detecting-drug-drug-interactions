    <sentence id="DDI-DrugBank.d412.s4" text="Cimetidine has been shown to increase the bioavailability of labetalol HCl.">
        <entity charOffset="0-9" id="DDI-DrugBank.d412.s4.e0" text="Cimetidine" type="drug" />
        <entity charOffset="61-73" id="DDI-DrugBank.d412.s4.e1" text="labetalol HCl" type="drug" />
