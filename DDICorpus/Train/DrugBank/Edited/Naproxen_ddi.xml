<document id="DDI-DrugBank.d85">
    <sentence id="DDI-DrugBank.d85.s0" text="The use of NSAIDs in patients who are receiving ACE inhibitors may potentiate renal disease states.">
        <entity charOffset="11-16" id="DDI-DrugBank.d85.s0.e0" text="NSAIDs" type="group" />
        <entity charOffset="48-61" id="DDI-DrugBank.d85.s0.e1" text="ACE inhibitors" type="group" />
        <pair ddi="true" e1="DDI-DrugBank.d85.s0.e0" e2="DDI-DrugBank.d85.s0.e1" id="DDI-DrugBank.d85.s0.p0" type="effect" />
    </sentence>
    <sentence id="DDI-DrugBank.d85.s8" text="Inhibition of renal lithium clearance leading to increases in plasma lithium concentrations has also been reported.">
        <entity charOffset="20-26" id="DDI-DrugBank.d85.s8.e0" text="lithium" type="drug" />
        <entity charOffset="69-75" id="DDI-DrugBank.d85.s8.e1" text="lithium" type="drug" />
        <pair ddi="false" e1="DDI-DrugBank.d85.s8.e0" e2="DDI-DrugBank.d85.s8.e1" id="DDI-DrugBank.d85.s8.p0" />
    </sentence>
    <sentence id="DDI-DrugBank.d85.s10" text="Probenecid given concurrently increases naproxen anion plasma levels and extends its plasma half-life significantly.">
        <entity charOffset="0-9" id="DDI-DrugBank.d85.s10.e0" text="Probenecid" type="drug" />
        <entity charOffset="40-47" id="DDI-DrugBank.d85.s10.e1" text="naproxen" type="drug" />
        <pair ddi="true" e1="DDI-DrugBank.d85.s10.e0" e2="DDI-DrugBank.d85.s10.e1" id="DDI-DrugBank.d85.s10.p0" type="mechanism" />
    </sentence>
    <sentence id="DDI-DrugBank.d85.s11" text="Caution should be used if naproxen is administered concomitantly with methotrexate.">
        <entity charOffset="26-33" id="DDI-DrugBank.d85.s11.e0" text="naproxen" type="drug" />
        <entity charOffset="70-81" id="DDI-DrugBank.d85.s11.e1" text="methotrexate" type="drug" />
