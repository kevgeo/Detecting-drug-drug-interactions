<document id="DDI-DrugBank.d97">
    <sentence id="DDI-DrugBank.d97.s3" text="Drugs that induce CYP3A4 activity would be expected to increase the clearance of indinavir, resulting in lowered plasma concentrations of indinavir.">
        <entity charOffset="81-89" id="DDI-DrugBank.d97.s3.e0" text="indinavir" type="drug" />
        <entity charOffset="138-146" id="DDI-DrugBank.d97.s3.e1" text="indinavir" type="drug" />
        <pair ddi="false" e1="DDI-DrugBank.d97.s3.e0" e2="DDI-DrugBank.d97.s3.e1" id="DDI-DrugBank.d97.s3.p0" />
    </sentence>
    <sentence id="DDI-DrugBank.d97.s8" text="Antiarrhythmics: amiodarone">
        <entity charOffset="0-14" id="DDI-DrugBank.d97.s8.e0" text="Antiarrhythmics" type="group" />
        <entity charOffset="17-26" id="DDI-DrugBank.d97.s8.e1" text="amiodarone" type="drug" />
