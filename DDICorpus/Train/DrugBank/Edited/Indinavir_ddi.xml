    </sentence>
    <sentence id="DDI-DrugBank.d97.s70" text="HMG-CoA Reductase Inhibitor: atorvastatin">
        <entity charOffset="0-26" id="DDI-DrugBank.d97.s70.e0" text="HMG-CoA Reductase Inhibitor" type="group" />
        <entity charOffset="29-40" id="DDI-DrugBank.d97.s70.e1" text="atorvastatin" type="drug" />
