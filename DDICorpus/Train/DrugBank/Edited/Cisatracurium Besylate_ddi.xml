    <sentence id="DDI-DrugBank.d60.s0" text="Administration of 0.1-mg/kg (2 x ED95) NIMBEX at 10% or 95% recovery following an intubating dose of succinylcholine (1 mg/kg) produced  95% neuromuscular block.">
        <entity charOffset="39-44" id="DDI-DrugBank.d60.s0.e0" text="NIMBEX" type="brand" />
        <entity charOffset="101-115" id="DDI-DrugBank.d60.s0.e1" text="succinylcholine" type="drug" />
