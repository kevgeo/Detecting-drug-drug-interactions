<document id="DDI-DrugBank.d87">
    <sentence id="DDI-DrugBank.d87.s4" text="In this formal interaction study, TAMBOCOR and propranolol were each found to have negative inotropic effects;">
        <entity charOffset="34-41" id="DDI-DrugBank.d87.s4.e0" text="TAMBOCOR" type="brand" />
        <entity charOffset="47-57" id="DDI-DrugBank.d87.s4.e1" text="propranolol" type="drug" />
        <pair ddi="false" e1="DDI-DrugBank.d87.s4.e0" e2="DDI-DrugBank.d87.s4.e1" id="DDI-DrugBank.d87.s4.p0" />
    </sentence>
    <sentence id="DDI-DrugBank.d87.s6" text="The effects of concomitant administration of TAMBOCOR and propranolol on the PR interval were less than additive.">
        <entity charOffset="45-52" id="DDI-DrugBank.d87.s6.e0" text="TAMBOCOR" type="brand" />
        <entity charOffset="58-68" id="DDI-DrugBank.d87.s6.e1" text="propranolol" type="drug" />
        <pair ddi="true" e1="DDI-DrugBank.d87.s6.e0" e2="DDI-DrugBank.d87.s6.e1" id="DDI-DrugBank.d87.s6.p0" type="effect" />
    </sentence>
    <sentence id="DDI-DrugBank.d87.s7" text="In TAMBOCOR clinical trials, patients who were receiving beta blockers concurrently did not experience an increased incidence of side effects.">
        <entity charOffset="3-10" id="DDI-DrugBank.d87.s7.e0" text="TAMBOCOR" type="brand" />
        <entity charOffset="57-69" id="DDI-DrugBank.d87.s7.e1" text="beta blockers" type="group" />
        <pair ddi="false" e1="DDI-DrugBank.d87.s7.e0" e2="DDI-DrugBank.d87.s7.e1" id="DDI-DrugBank.d87.s7.p0" />
    </sentence>
    <sentence id="DDI-DrugBank.d87.s8" text="Nevertheless, the possibility of additive negative inotropic effects of beta blockers and flecainide should be recognized.">
        <entity charOffset="72-84" id="DDI-DrugBank.d87.s8.e0" text="beta blockers" type="group" />
        <entity charOffset="90-99" id="DDI-DrugBank.d87.s8.e1" text="flecainide" type="drug" />
        <pair ddi="true" e1="DDI-DrugBank.d87.s8.e0" e2="DDI-DrugBank.d87.s8.e1" id="DDI-DrugBank.d87.s8.p0" type="effect" />
    </sentence>
    <sentence id="DDI-DrugBank.d87.s12" text="TAMBOCOR has been used in a large number of patients receiving diuretics without apparent interaction.">
        <entity charOffset="0-7" id="DDI-DrugBank.d87.s12.e0" text="TAMBOCOR" type="brand" />
        <entity charOffset="63-71" id="DDI-DrugBank.d87.s12.e1" text="diuretics" type="group" />
        <pair ddi="false" e1="DDI-DrugBank.d87.s12.e0" e2="DDI-DrugBank.d87.s12.e1" id="DDI-DrugBank.d87.s12.p0" />
    </sentence>
    <sentence id="DDI-DrugBank.d87.s14" text="In healthy subjects receiving cimetidine (1 gm daily) for one week, plasma flecainide levels increased by about 30% and half-life increased by about 10%.">
        <entity charOffset="30-39" id="DDI-DrugBank.d87.s14.e0" text="cimetidine" type="drug" />
        <entity charOffset="75-84" id="DDI-DrugBank.d87.s14.e1" text="flecainide" type="drug" />
        <pair ddi="true" e1="DDI-DrugBank.d87.s14.e0" e2="DDI-DrugBank.d87.s14.e1" id="DDI-DrugBank.d87.s14.p0" type="mechanism" />
    </sentence>
