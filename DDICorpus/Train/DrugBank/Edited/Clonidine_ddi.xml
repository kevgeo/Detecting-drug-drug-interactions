<document id="DDI-DrugBank.d495">
    <sentence id="DDI-DrugBank.d495.s3" text="Narcotic analgesics may potentiate the hypotensive effects of clonidine.">
        <entity charOffset="0-18" id="DDI-DrugBank.d495.s3.e0" text="Narcotic analgesics" type="group" />
        <entity charOffset="62-70" id="DDI-DrugBank.d495.s3.e1" text="clonidine" type="drug" />
        <pair ddi="true" e1="DDI-DrugBank.d495.s3.e0" e2="DDI-DrugBank.d495.s3.e1" id="DDI-DrugBank.d495.s3.p0" type="effect" />
    </sentence>
    <sentence id="DDI-DrugBank.d495.s4" text="Tricyclic antidepressants may antagonize the hypotensive effects of clonidine.">
        <entity charOffset="0-24" id="DDI-DrugBank.d495.s4.e0" text="Tricyclic antidepressants" type="group" />
        <entity charOffset="68-76" id="DDI-DrugBank.d495.s4.e1" text="clonidine" type="drug" />
        <pair ddi="true" e1="DDI-DrugBank.d495.s4.e0" e2="DDI-DrugBank.d495.s4.e1" id="DDI-DrugBank.d495.s4.p0" type="effect" />
    </sentence>
    <sentence id="DDI-DrugBank.d495.s5" text="The effects of tricyclic antidepressants on clonidines analgesic actions are not known.">
        <entity charOffset="15-39" id="DDI-DrugBank.d495.s5.e0" text="tricyclic antidepressants" type="group" />
        <entity charOffset="44-52" id="DDI-DrugBank.d495.s5.e1" text="clonidine" type="drug" />
        <pair ddi="false" e1="DDI-DrugBank.d495.s5.e0" e2="DDI-DrugBank.d495.s5.e1" id="DDI-DrugBank.d495.s5.p0" />
    </sentence>
    <sentence id="DDI-DrugBank.d495.s6" text="Beta blockers may exacerbate the hypertensive response seen with clonidine withdrawl.">
        <entity charOffset="0-12" id="DDI-DrugBank.d495.s6.e0" text="Beta blockers" type="group" />
        <entity charOffset="65-73" id="DDI-DrugBank.d495.s6.e1" text="clonidine" type="drug" />
        <pair ddi="true" e1="DDI-DrugBank.d495.s6.e0" e2="DDI-DrugBank.d495.s6.e1" id="DDI-DrugBank.d495.s6.p0" type="effect" />
    </sentence>
    <sentence id="DDI-DrugBank.d495.s8" text="There is one reported case of a patient with acute delirium associated with the simultaneous use of fluphenazine and oral clonidine.">
        <entity charOffset="100-111" id="DDI-DrugBank.d495.s8.e0" text="fluphenazine" type="drug" />
        <entity charOffset="122-130" id="DDI-DrugBank.d495.s8.e1" text="clonidine" type="drug" />
        <pair ddi="true" e1="DDI-DrugBank.d495.s8.e0" e2="DDI-DrugBank.d495.s8.e1" id="DDI-DrugBank.d495.s8.p0" type="effect" />
    </sentence>
    <sentence id="DDI-DrugBank.d495.s9" text="Symptoms resolved when clonidine was withdrawn and recurred when the patient was rechallenged with clonidine.">
        <entity charOffset="23-31" id="DDI-DrugBank.d495.s9.e0" text="clonidine" type="drug" />
        <entity charOffset="99-107" id="DDI-DrugBank.d495.s9.e1" text="clonidine" type="drug" />
        <pair ddi="false" e1="DDI-DrugBank.d495.s9.e0" e2="DDI-DrugBank.d495.s9.e1" id="DDI-DrugBank.d495.s9.p0" />
    </sentence>
    <sentence id="DDI-DrugBank.d495.s10" text="Epidural clonidine may prolong the duration of pharmacologic effects of epidural local anesthetics, including both sensory and motor blockade.">
        <entity charOffset="9-17" id="DDI-DrugBank.d495.s10.e0" text="clonidine" type="drug" />
        <entity charOffset="87-97" id="DDI-DrugBank.d495.s10.e1" text="anesthetics" type="group" />
        <pair ddi="true" e1="DDI-DrugBank.d495.s10.e0" e2="DDI-DrugBank.d495.s10.e1" id="DDI-DrugBank.d495.s10.p0" type="effect" />
    </sentence>
