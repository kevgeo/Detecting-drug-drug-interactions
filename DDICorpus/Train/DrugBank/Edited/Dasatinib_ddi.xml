    <sentence id="DDI-DrugBank.d48.s0" text="Drugs that may increase dasatinib plasma concentrations CYP3A4 Inhibitors: Dasatinib is a CYP3A4 substrate.">
        <entity charOffset="24-32" id="DDI-DrugBank.d48.s0.e0" text="dasatinib" type="drug" />
        <entity charOffset="75-83" id="DDI-DrugBank.d48.s0.e1" text="Dasatinib" type="drug" />
