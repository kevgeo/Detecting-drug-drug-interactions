    </sentence>
    <sentence id="DDI-DrugBank.d48.s8" text="Antacids: Nonclinical data demonstrate that the solubility of dasatinib is pH dependent.">
        <entity charOffset="0-7" id="DDI-DrugBank.d48.s8.e0" text="Antacids" type="group" />
        <entity charOffset="62-70" id="DDI-DrugBank.d48.s8.e1" text="dasatinib" type="drug" />
