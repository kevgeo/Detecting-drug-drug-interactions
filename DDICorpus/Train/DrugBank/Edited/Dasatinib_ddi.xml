    </sentence>
    <sentence id="DDI-DrugBank.d48.s3" text="Drugs that may decrease dasatinib plasma concentrations CYP3A4 Inducers: Drugs that induce CYP3A4 activity may decrease dasatinib plasma concentrations.">
        <entity charOffset="24-32" id="DDI-DrugBank.d48.s3.e0" text="dasatinib" type="drug" />
        <entity charOffset="120-128" id="DDI-DrugBank.d48.s3.e1" text="dasatinib" type="drug" />
        <pair ddi="false" e1="DDI-DrugBank.d48.s3.e0" e2="DDI-DrugBank.d48.s3.e1" id="DDI-DrugBank.d48.s3.p0" />
    </sentence>
    <sentence id="DDI-DrugBank.d48.s5" text="If SPRYCEL must be administered with a CYP3A4 inducer, a dose increase in SPRYCEL should be considered.">
        <entity charOffset="3-9" id="DDI-DrugBank.d48.s5.e0" text="SPRYCEL" type="brand" />
        <entity charOffset="74-80" id="DDI-DrugBank.d48.s5.e1" text="SPRYCEL" type="brand" />
