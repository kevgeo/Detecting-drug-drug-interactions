<document id="DDI-DrugBank.d83">
    <sentence id="DDI-DrugBank.d83.s0" text="Patients on warfarin-type anticoagulant therapy may require dosage adjustment of the anticoagulant during and after griseofulvin therapy.">
        <entity charOffset="12-38" id="DDI-DrugBank.d83.s0.e0" text="warfarin-type anticoagulant" type="group" />
        <entity charOffset="116-127" id="DDI-DrugBank.d83.s0.e1" text="griseofulvin" type="drug" />
        <pair ddi="true" e1="DDI-DrugBank.d83.s0.e0" e2="DDI-DrugBank.d83.s0.e1" id="DDI-DrugBank.d83.s0.p0" type="advise" />
    </sentence>
    <sentence id="DDI-DrugBank.d83.s1" text="Concomitant use of barbiturates usually depresses griseofulvin activity and may necessitate raising the dosage.">
        <entity charOffset="19-30" id="DDI-DrugBank.d83.s1.e0" text="barbiturates" type="group" />
        <entity charOffset="50-61" id="DDI-DrugBank.d83.s1.e1" text="griseofulvin" type="drug" />
        <pair ddi="true" e1="DDI-DrugBank.d83.s1.e0" e2="DDI-DrugBank.d83.s1.e1" id="DDI-DrugBank.d83.s1.p0" type="effect" />
    </sentence>
    <sentence id="DDI-DrugBank.d83.s2" text="The concomitant administration of griseofulvin has been reported to reduce the efficacy of oral contraceptives and to increase the incidence of breakthrough bleeding.">
        <entity charOffset="34-45" id="DDI-DrugBank.d83.s2.e0" text="griseofulvin" type="drug" />
