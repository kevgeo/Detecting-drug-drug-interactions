    <sentence id="DDI-DrugBank.d64.s0" text="Addition or deletion of any drug from the therapeutic regimen of patients receiving oral anticoagulants may affect patient response to the anticoagulant.">
        <entity charOffset="89-102" id="DDI-DrugBank.d64.s0.e0" text="anticoagulants" type="group" />
