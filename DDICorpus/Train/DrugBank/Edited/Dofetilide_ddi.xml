    </sentence>
    <sentence id="DDI-DrugBank.d558.s6" text="Verapamil: Concomitant use of verapamil is contraindicated.">
        <entity charOffset="0-8" id="DDI-DrugBank.d558.s6.e0" text="Verapamil" type="drug" />
        <entity charOffset="30-38" id="DDI-DrugBank.d558.s6.e1" text="verapamil" type="drug" />
