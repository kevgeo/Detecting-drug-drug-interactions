    <sentence id="DDI-DrugBank.d558.s1" text="Drug-Drug Interactions Cimetidine: Concomitant use of cimetidine is contraindicated.">
        <entity charOffset="23-32" id="DDI-DrugBank.d558.s1.e0" text="Cimetidine" type="drug" />
