    <sentence id="DDI-DrugBank.d368.s6" text="Acetazolamide may increase the effects of other folic acid antagonists.">
        <entity charOffset="0-12" id="DDI-DrugBank.d368.s6.e0" text="Acetazolamide" type="drug" />
