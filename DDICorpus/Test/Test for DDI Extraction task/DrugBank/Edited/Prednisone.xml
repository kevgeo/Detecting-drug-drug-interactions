    <sentence id="DDI-DrugBank.d653.s3" text="Therefore, the dose of corticosteroid should be titrated to avoid steroid toxicity. ">
        <entity charOffset="23-36" id="DDI-DrugBank.d653.s3.e0" text="corticosteroid" type="group" />
    </sentence>
    <sentence id="DDI-DrugBank.d653.s4" text="Corticosteroids may increase the clearance of chronic high dose aspirin. ">
        <entity charOffset="0-14" id="DDI-DrugBank.d653.s4.e0" text="Corticosteroids" type="group" />
        <entity charOffset="64-70" id="DDI-DrugBank.d653.s4.e1" text="aspirin" type="brand" />
