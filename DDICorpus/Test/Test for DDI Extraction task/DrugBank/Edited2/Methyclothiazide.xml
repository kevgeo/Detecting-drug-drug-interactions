    </sentence>
    <sentence id="DDI-DrugBank.d736.s3" text="Thiazides may decrease arterial responsiveness to norepinephrine. ">
        <entity charOffset="0-8" id="DDI-DrugBank.d736.s3.e0" text="Thiazides" type="group" />
        <entity charOffset="50-63" id="DDI-DrugBank.d736.s3.e1" text="norepinephrine" type="drug" />
